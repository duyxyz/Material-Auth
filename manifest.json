{
  "manifest_version": 3,
  "name": "Material Auth",
  "version": "1.1",
  "description": "Open-source 2FA Authenticator built with Material Design. Secure, private, and compatible with Google Authenticator migration URIs.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  }
}